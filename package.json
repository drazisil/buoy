{
  "name": "buoy",
  "version": "0.0.1",
  "description": "",
  "bin": {
    "buoy": "./bin/cli.js"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "type": "module",
  "scripts": {
    "clean": "rm -rf tmp/** && touch tmp/.gitkeep",
    "docs": "rm -rf out && ./node_modules/.bin/jsdoc -t jsdoc-template -c jsdocconf.json --verbose -R README.md -r .",
    "test": "xo && c8 -x jsdoc-template -x out -x test -x coverage -r text -r cobertura -r html --all node test/loader.js"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/node": "18.7.13",
    "c8": "7.12.0",
    "jsdoc": "3.6.11",
    "typescript": "4.8.3",
    "xo": "0.52.3"
  },
  "volta": {
    "node": "18.8.0"
  },
  "xo": {
    "ignores": [
      "jsdoc-template"
    ],
    "rules": {
      "n/prefer-global/process": [
        "error",
        "always"
      ],
      "n/prefer-global/buffer": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "prompts": "2.4.2",
    "sanitize-filename": "^1.6.3",
    "tar-fs": "2.1.1",
    "undici": "5.10.0"
  }
}
