{
  "name": "buoy",
  "version": "0.0.1",
  "description": "",
  "bin": {
    "buoy": "./built/cli.js"
  },
  "exports": {
    ".": "./src/index.js"
  },
  "type": "module",
  "scripts": {
    "build": "xo --fix && tsc --build",
    "clean": "rm -rf ~/.buoy/**",
    "docs": "rm -rf out && ./node_modules/.bin/jsdoc -t jsdoc-template -c jsdocconf.json --verbose -R README.md -r .",
    "test": "xo && c8 -x jsdoc-template -x out -x test -x built -x coverage -r text -r cobertura -r html --all mocha"
  },
  "keywords": [],
  "author": "",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/debug": "^4.1.7",
    "@types/mocha": "9.1.1",
    "@types/nock": "^11.1.0",
    "@types/node": "18.7.13",
    "@types/prompts": "2.0.14",
    "@types/tar-fs": "2.0.1",
    "c8": "7.12.0",
    "i": "0.3.7",
    "jsdoc": "3.6.11",
    "mocha": "10.0.0",
    "nock": "^13.2.9",
    "npm": "8.19.2",
    "ts-node": "10.9.1",
    "typescript": "4.8.3",
    "xo": "0.52.3"
  },
  "volta": {
    "node": "18.8.0"
  },
  "xo": {
    "ignores": [
      "jsdoc-template",
      "built"
    ],
    "rules": {
      "@typescript-eslint/naming-convention": [
        "off"
      ],
      "n/prefer-global/process": [
        "error",
        "always"
      ],
      "n/prefer-global/buffer": [
        "error",
        "always"
      ]
    }
  },
  "dependencies": {
    "debug": "^4.3.4",
    "prompts": "2.4.2",
    "sanitize-filename": "^1.6.3",
    "tar-fs": "2.1.1",
    "undici": "5.10.0"
  }
}
